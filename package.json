{
  "name": "github-release-packager",
  "version": "0.0.3",
  "description": "Package releases of GitHub projects.",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run build",
    "test": "node ./node_modules/mocha/bin/_mocha --timeout 999999 --colors ./test/_suite.js",
    "ca": "semistandard",
    "prebuild": "npm run ca",
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "tsc -p tsconfig.json"
  },
  "keywords": [
    "npm",
    "github",
    "javascript"
  ],
  "author": "Thorben Wolkersdorfer <thorbenw@nbb.com> (https://www.notebooksbilliger.de/)",
  "license": "MIT",
  "repository": "github:thorbenw/github-release-packager",
  "readme": "README.md",
  "engines": {
    "node": ">=8.17.0"
  },
  "directories": {
    "lib": "./lib",
    "test": "./test"
  },
  "dependencies": {
    "callsites": "^3.1.0",
    "deasync": "^0.1.19",
    "fs-extra": "^8.0.0",
    "request": "^2.88.2",
    "semver": "^7.1.3",
    "then-request": "^6.0.2",
    "unzipper": "^0.10.10"
  },
  "devDependencies": {
    "@types/deasync": "^0.1.0",
    "@types/fs-extra": "^8.1.0",
    "@types/mocha": "^7.0.2",
    "@types/request": "^2.48.4",
    "@types/semver": "^7.1.0",
    "@types/unzipper": "^0.10.3",
    "mocha": "^7.1.1"
  },
  "semistandard": {
    "env": "mocha"
  }
}
